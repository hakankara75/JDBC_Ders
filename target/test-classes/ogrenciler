  CREATE TABLE ogrenciler (
  ogrenci_id INT,
  ogrenci_adi VARCHAR(50),
  ogrenci_soyadi VARCHAR(50),
  ogrenci_no INT,
  ogrenci_bolumu VARCHAR(50),
  ogrenci_dogum_tarihi DATE,
  ogrenci_ortalama REAL
);


